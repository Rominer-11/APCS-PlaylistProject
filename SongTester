



public class SongTester 
{

    public static void main(String[] args) 
    {
        
        Song Goosebumps = new Song("Goosebumps", 180, "Travis Scott");

        
        System.out.println("Name: " + Goosebumps.getName());
        System.out.println("Artist: " + Goosebumps.getArtist());
        System.out.println("Duration: " + Goosebumps.getLength());
        System.out.println("Liked: " + Goosebumps.getLiked());

        // Like/Unlike the song and display the updated liked status
        Goosebumps.LikeButton();
        System.out.println("Liked: " + Goosebumps.getLiked());

        // Display the full song information using toString method
        System.out.println(Goosebumps.toString());
    }
}

